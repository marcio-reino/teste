generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  uuid      String   @unique
  name      String
  cellphone String
  email     String   @unique
  status    Boolean  @default(true)
}
